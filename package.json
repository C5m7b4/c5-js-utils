{
  "name": "c5-js-utils",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "testWatch": "mocha src/**/*.js --watch",
    "test": "mocha src/**/*.js",
    "test:coverage":"npm run cover:unit && npm run cover:report",
    "test:coveragehtml":"npm run cover:unit && npm run cover:reporthtml",
    "cover:unit":"nyc --silent npm run test",
    "cover:report":"nyc report --reporter=lcov --reporter=text --report-dir='./coverage'",
    "cover:reporthtml":"nyc report --reporter-html --report-dir='./coverage'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1"
  }
}
